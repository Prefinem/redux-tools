{
	"author": {
		"email": "prefinem@gmail.com",
		"name": "Prefinem",
		"url": "https://prefinem.com"
	},
	"description": "Redux Tools to make redux easy",
	"devDependencies": {
		"@babel/cli": "7.8.3",
		"@babel/core": "7.8.3",
		"@babel/plugin-proposal-object-rest-spread": "7.8.3",
		"@babel/preset-react": "7.8.3",
		"babel-eslint": "10.0.3",
		"eslint": "6.8.0",
		"eslint-config-prettier": "6.9.0",
		"eslint-plugin-babel": "5.3.0",
		"eslint-plugin-jest": "23.6.0",
		"eslint-plugin-react": "7.17.0",
		"husky": "4.0.7",
		"jest": "24.9.0",
		"lint-staged": "9.5.0",
		"prettier": "1.19.1"
	},
	"homepage": "https://github.com/Prefinem/redux-tools",
	"husky": {
		"hooks": {
			"pre-commit": "yarn staged"
		}
	},
	"license": "MIT",
	"lint-staged": {
		"*.js": [
			"eslint --fix",
			"prettier --config ./.prettierrc.json --write",
			"jest --bail --findRelatedTests",
			"git add"
		]
	},
	"main": "./index.js",
	"name": "redux-tools",
	"peerDependencies": {
		"isomorphic-fetch": ">=2",
		"redux": ">=4",
		"redux-saga": ">=0.16"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Prefinem/redux-tools"
	},
	"resolutions": {
		"js-yaml": "3.13.1",
		"mem": "4.0.0",
		"webpack-dev-server": "3.1.11"
	},
	"scripts": {
		"build": "rm -rf dist/ && NODE_ENV=production ./node_modules/.bin/babel src/ --out-dir dist",
		"clean": "yarn lint && yarn prettify",
		"eslint-check": "eslint --print-config .eslintrc.json | eslint-config-prettier-check",
		"lint": "eslint src/** --fix",
		"prettify": "prettier --config ./.prettierrc.json --write \"src/**\"",
		"staged": "lint-staged",
		"test": "jest --coverage"
	},
	"version": "3.2.0"
}
